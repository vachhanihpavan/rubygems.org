<%= render "ownership_requests/ownership_request", ownership_request: ownership_request do %>
  <%= button_to "Approve", rubygem_ownership_request_path(@rubygem, ownership_request), params: { status: "approve" }, method: :patch, class: "form__submit form__submit--medium" %>
  <%= button_to "Close", rubygem_ownership_request_path(@rubygem, ownership_request), params: { status: "close" }, method: :patch, class: "form__submit form__submit--medium" %>
<% end %>
